{
    "name": "dropwow-nova/add-product-card",
    "description": "A Dropwow Nova card.",
    "keywords": [
        "dropwow",
        "nova",
        "ali"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Dropwow\\AddProduct\\": "src/"
        }
    },
    "extra": {
        "laravel": {
